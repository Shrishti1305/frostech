import java.util.Scanner;

public class program6 {
    public static void main (String[]args){
Scanner s = new Scanner(System.in);
 int a = s.nextInt();
 int b = s.nextInt();
 
char op = s.next().charAt(0);
if (op == '+' ){
    System.out.println(a+b);
}else if (op == '-'){
    System.out.println(a-b);;
} else if(op == '*'){
    System.out.println(a*b);
}


}
}
