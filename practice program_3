public class palindrome {
    public static void main (String[]args){
     int n=  65456;int r;int sum=0;int temp;
     temp = n;
     r = n%10;
     sum = (sum*10)+r;
     n = n/10;
     if (temp==sum){
        System.out.println("its is a palindrome");
     }
     else{
        System.out.println("not a palindrome");
     }





    }
    
}
