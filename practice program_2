public class fibonacciseries {
    public static void main (String[]args){
    // fibonacci series: consist of 0 and 1 in the beginning and further digits are the sum of previous ones
    int n1 = 0 ; int n2 = 1 ;int i;
    System.out.println( n1+" "+n2);// the syntax n1+" "+n2 is the concatination series which
        for(i = 2 ; i < 10 ; i++){
      int n3 ;
      n3 = n1+n2 ;
       n1=n2;
       n2=n3;
       System.out.print(" "+n3);


        }
        

    
    }
    
}
